{
    "openapi": "3.0.0",
    "info": {
        "title": "ApiWeb",
        "version": "1.0"
    },
    "paths": {
        "/auth/signin": {
            "post": {
                "tags": [
                    "Oauth"
                ],
                "operationId": "Signin",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "username": {
                                        "description": "Usuario",
                                        "type": "string",
                                        "example": "wordpress"
                                    },
                                    "password": {
                                        "description": "Contrase√±a",
                                        "type": "string",
                                        "example": "wordpress"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Retorna Usuario Autenticado"
                    },
                    "400": {
                        "description": "Error: Error de Login."
                    }
                }
            }
        }
    }
}